{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode": false,
    "declaration": false,
    "experimentalDecorators": true,
    "lib": [
      "dom",
      "es2015",
      "webworker",
    ],
    "moduleResolution": "node",
    "module": "esnext",
    "target": "es2017",
    "noUnusedLocals": true,
    "noUnusedParameters": true,    
    "skipLibCheck": true
  },
  "include": [
    "src/sw.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
